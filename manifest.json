{
  "manifest_version": 3,
  "name": "Simple Popup History",
  "version": "1.0.0",
  "description": "Chrome の閲覧履歴を検索可能な一覧で表示します",
  "permissions": [
    "history"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
